
#include<iostream>
using namespace std;
int main()
{
    int row,col,i,j,arr[10][10];
    cin>>row>>col;
    for(i=0;i<=row-1;i++)
        for(j=0;j<=col-1;j++)
            cin>>arr[i][j];

    for(j=0;j<=col-1;j++)
        if(j%2==0)
    {
        for(i=0;i<=row-1;i++)
            cout<<arr[i][j]<<", ";

    }
    else
    {
        for(i=row-1;i>=0;i--)
            cout<<arr[i][j]<<", ";

    }
    cout<<"END";





}
