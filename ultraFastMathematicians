
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int test;
    cin>>test;
	string s1,s2,s3;

    while(test>0)
    {
        //s3="";
        cin>>s1>>s2;
        for(int i=0;s1[i]!='\0';i++)
            if(s1[i]==s2[i])
                s3[i]='0';
            else
                s3[i]='1';
            //cout<<s3<<endl;
            for(int i=0;s1[i]!='\0';i++)
            {
                cout<<s3[i];
            }
            cout<<endl;
        test--;
    }
}
